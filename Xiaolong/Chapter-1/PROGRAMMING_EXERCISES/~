
class Rectangle {
   static DecimalFormat df1 = new DecimalFormat( "#.##" );
   public static void main(String[] args) {
        double width = 4.5, height = 7.9;
  
        double area  = width * height;
        double area0 = df1.format(area); 
	double area1 = (double)((int)(width * height * 100) / 100);
        System.out.println(area + " " + area0 + " " + area1);
   }
}
